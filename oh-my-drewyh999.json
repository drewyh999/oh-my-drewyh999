{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "console_title_template": "{{.Folder}}{{if .Root}} :: root{{end}} :: {{.Shell}}",
  "version": 2,
  "foreground": "#0c991c",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "foreground": "#ffffff",
          "background": "p:c-hostname",
          "template": "\uF197  {{ .HostName }} "
        },
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "\uE0b0",
          "foreground": "#1404f6",
          "background": "#ffffff",
          "template": " {{ .Icon }} ",
          "properties": {
            "macos": "\uf535"
          }
        },
        {
          "type": "text",
          "style": "powerline",
          "powerline_symbol": "\uE0b0",
          "foreground": "#000000",
          "background": "#e98126",
          "template": " \uf007 {{ .UserName }}"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "p:c-folder-foreground",
          "background": "p:c-badge-folder",
          "properties": {
            "style": "agnoster"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#100e23",
          "background": "#42c96f",
          "template": " \uf51a {{ .Venv }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "az",
          "style": "diamond",
          "leading_diamond": "\uE0c5",
          "trailing_diamond": "\uE0c4",
          "foreground": "#100e23",
          "background": "#1ed6e3",
          "template": " \ufd03 Subscription:{{.Name}} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#FFFB38",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#C792EA{{ end }}"
          ],
          "foreground": "#011627",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "type": "git"
        },
        {
          "type": "text",
          "style": "plain",
          "leading_diamond": "\uE0b6",
          "trailing_diamond": "\uE0b0",
          "foreground": "#ffffff",
          "template": " \uf989"
        }
      ]
    }
  ],
  "palette": {
    "c-badge-folder": "#5a32a5",
    "c-hostname": "#1404f6",
    "c-folder-foreground": "#ffffff"
  },
  "transient_prompt": {
    "template": "{{ if eq \"False\" (title (default \"False\" .Env.DISABLE_SEGMENT_TRANSIENT)) }}<p:c-badge-folder>\uE0B6</><#ffffff,p:c-badge-folder><b>{{.Shell}}</b></><#ffffff,p:c-badge-folder> \uf101</><p:c-badge-folder>\uE0B4</> {{ end }}"
  }
}